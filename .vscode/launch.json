{
  // Use IntelliSense to learn about possible Node.js debug attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    // Debug server
    {
      "name": "Debug Auto-Subliminal",
      "type": "python",
      "request": "launch",
      "program": "AutoSubliminal.py",
      "console": "integratedTerminal"
    },
    // Debug ui
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Debug ui",
      "url": "http://localhost:8084", // Adapt port to the port your python server is running on
      "webRoot": "${workspaceFolder}/web/autosubliminal"
    },
    // Debug ui (dev)
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Debug ui (dev)",
      "url": "http://localhost:4200/autosubliminal", // Adapt port to the port your python server is running on
      "webRoot": "${workspaceFolder}/web/autosubliminal"
    }
  ]
}
