{
  "extends": [
    "config:base"
  ],
  "baseBranches": [
    "development"
  ],
  "js": {
    "labels": [
      "Update dep (JavaScript)"
    ],
    "packageRules": [
      {
        "packageNames": [
          "bootstrap"
        ],
        "allowedVersions": "<4.0.0"
      }
    ]
  },
  "pip_requirements": {
    "fileMatch": [
      "requirements/libraries.txt"
    ]
  },
  "python": {
    "labels": [
      "Update dep (Python)"
    ],
    "packageRules": [
      {
        "packageNames": [
          "cherrypy"
        ],
        "allowedVersions": "<18.0.0"
      },
      {
        "packageNames": [
          "more-itertools"
        ],
        "allowedVersions": "<6.0.0"
      }
    ]
  }
}
