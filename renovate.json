{
  "extends": [
    "config:base"
  ],
  "baseBranches": [
    "development"
  ],
  "enabledManagers": [
    "npm",
    "poetry"
  ],
  "npm": {
    "fileMatch": [
      "web/autosubliminal/package.json"
    ]
  },
  "js": {
    "labels": [
      "dependencies",
      "javascript"
    ],
    "packageRules": [
      {
        "packageNames": [
          "bootstrap"
        ],
        "allowedVersions": "<5.0.0"
      }
    ]
  },
  "poetry": {
    "fileMatch": [
      "pyproject.toml"
    ]
  },
  "python": {
    "labels": [
      "dependencies",
      "python"
    ],
    "packageRules": []
  }
}
