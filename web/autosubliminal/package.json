{
  "name": "autosubliminal",
  "description": "Auto-Subliminal, the automated subliminal subtitle downloader",
  "author": "h3llrais3r",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h3llrais3r/Auto-Subliminal.git"
  },
  "homepage": "https://github.com/h3llrais3r/Auto-Subliminal#readme",
  "bugs": {
    "url": "https://github.com/h3llrais3r/Auto-Subliminal/issues"
  },
  "keywords": [
    "subtitle",
    "subtitles",
    "video",
    "movie",
    "episode",
    "tv",
    "show"
  ],
  "scripts": {
    "clean": "rimraf static",
    "copy-themes": "concurrently -n w: npm:copy-themes-*",
    "copy-themes-bootstrap4": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/bootstrap4-*/theme.css\" ./src/assets/themes",
    "copy-themes-arya": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/arya-*/theme.css\" ./src/assets/themes",
    "copy-themes-saga": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/saga-*/theme.css\" ./src/assets/themes",
    "copy-themes-vela": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/vela-*/theme.css\" ./src/assets/themes",
    "build": "npm run clean && npm run copy-themes && ng build --base-href=/autosubliminal/ --prod",
    "start": "npm run copy-themes && ng serve --host 0.0.0.0 --base-href=/autosubliminal/ --proxy-config proxy.localhost.conf.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.2.5",
    "@angular/cdk": "10.2.7",
    "@angular/common": "10.2.5",
    "@angular/compiler": "10.2.5",
    "@angular/core": "10.2.5",
    "@angular/forms": "10.2.5",
    "@angular/platform-browser": "10.2.5",
    "@angular/platform-browser-dynamic": "10.2.5",
    "@angular/router": "10.2.5",
    "bootstrap": "4.6.1",
    "font-awesome": "4.7.0",
    "jquery": "3.6.0",
    "lodash-es": "4.17.21",
    "moment": "2.29.1",
    "natural-compare": "1.4.0",
    "ng-lazyload-image": "9.1.0",
    "ngx-countdown": "10.0.2",
    "ngx-mask": "9.1.4",
    "ngx-webstorage": "6.0.0",
    "primeicons": "4.1.0",
    "primeng": "10.0.3",
    "rxjs": "6.6.7",
    "strftime": "0.10.0",
    "tslib": "2.0.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1002.3",
    "@angular/cli": "10.2.3",
    "@angular/compiler-cli": "10.2.5",
    "@types/jasmine": "3.10.0",
    "@types/jasminewd2": "2.0.10",
    "@types/jquery": "3.5.8",
    "@types/lodash-es": "4.17.5",
    "@types/natural-compare": "1.4.1",
    "@types/node": "12.20.33",
    "@types/strftime": "0.9.4",
    "codelyzer": "6.0.2",
    "concurrently": "6.3.0",
    "copyfiles": "2.4.1",
    "jasmine-core": "3.10.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "protractor": "7.0.0",
    "rimraf": "3.0.2",
    "ts-node": "8.10.2",
    "tslint": "6.1.3",
    "typescript": "4.0.2"
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
