{
  "name": "autosubliminal",
  "description": "Auto-Subliminal, the automated subliminal subtitle downloader",
  "author": "h3llrais3r",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h3llrais3r/Auto-Subliminal.git"
  },
  "homepage": "https://github.com/h3llrais3r/Auto-Subliminal#readme",
  "bugs": {
    "url": "https://github.com/h3llrais3r/Auto-Subliminal/issues"
  },
  "keywords": [
    "subtitle",
    "subtitles",
    "video",
    "movie",
    "episode",
    "tv",
    "show"
  ],
  "scripts": {
    "prepare": "cd ../.. && husky install .githooks",
    "clean": "rimraf static",
    "copy-themes": "concurrently -n w: npm:copy-themes-*",
    "copy-themes-bootstrap4": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/bootstrap4-*/theme.css\" ./src/assets/themes",
    "copy-themes-arya": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/arya-*/**/*\" ./src/assets/themes",
    "copy-themes-lara": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/lara-*/**/*\" ./src/assets/themes",
    "copy-themes-saga": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/saga-*/**/*\" ./src/assets/themes",
    "copy-themes-vela": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/vela-*/**/*\" ./src/assets/themes",
    "fix-line-endings": "node fix-line-endings.js",
    "start": "npm run copy-themes && ng serve --host 0.0.0.0 --proxy-config proxy.localhost.conf.json",
    "build": "npm run clean && npm run copy-themes && ng build && npm run fix-line-endings",
    "build-ci": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "ng": "ng"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.1.8",
    "@angular/cdk": "16.2.0",
    "@angular/common": "16.1.8",
    "@angular/compiler": "16.1.8",
    "@angular/core": "16.1.8",
    "@angular/forms": "16.1.8",
    "@angular/platform-browser": "16.1.8",
    "@angular/platform-browser-dynamic": "16.1.8",
    "@angular/router": "16.1.8",
    "bootstrap": "4.6.2",
    "font-awesome": "4.7.0",
    "jquery": "3.7.0",
    "lodash-es": "4.17.21",
    "moment": "2.29.4",
    "natural-compare": "1.4.0",
    "ng-lazyload-image": "9.1.3",
    "ngx-countdown": "16.0.0",
    "ngx-mask": "16.2.8",
    "ngx-webstorage": "12.0.0",
    "ngx-window-token": "7.0.0",
    "primeicons": "6.0.1",
    "primeng": "16.1.0",
    "rxjs": "7.8.1",
    "strftime": "0.10.2",
    "tslib": "2.6.1",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.1.8",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "16.1.8",
    "@angular/compiler-cli": "16.1.8",
    "@types/jasmine": "4.3.5",
    "@types/jasminewd2": "2.0.10",
    "@types/jquery": "3.5.16",
    "@types/lodash-es": "4.17.8",
    "@types/natural-compare": "1.4.1",
    "@types/node": "16.18.40",
    "@types/strftime": "0.9.4",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "concurrently": "8.2.0",
    "copyfiles": "2.4.1",
    "eslint": "8.46.0",
    "husky": "8.0.3",
    "jasmine-core": "4.6.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.2",
    "karma-jasmine-html-reporter": "1.7.0",
    "line-ending-corrector": "1.0.1",
    "rimraf": "5.0.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": "16.20.2"
  },
  "volta": {
    "node": "16.20.2"
  }
}
