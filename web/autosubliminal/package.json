{
  "name": "autosubliminal",
  "description": "Auto-Subliminal, the automated subliminal subtitle downloader",
  "author": "h3llrais3r",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h3llrais3r/Auto-Subliminal.git"
  },
  "homepage": "https://github.com/h3llrais3r/Auto-Subliminal#readme",
  "bugs": {
    "url": "https://github.com/h3llrais3r/Auto-Subliminal/issues"
  },
  "keywords": [
    "subtitle",
    "subtitles",
    "video",
    "movie",
    "episode",
    "tv",
    "show"
  ],
  "scripts": {
    "prepare": "cd ../.. && husky install .githooks",
    "clean": "rimraf static",
    "copy-themes": "concurrently -n w: npm:copy-themes-*",
    "copy-themes-bootstrap4": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/bootstrap4-*/theme.css\" ./src/assets/themes",
    "copy-themes-arya": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/arya-*/**/*\" ./src/assets/themes",
    "copy-themes-lara": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/lara-*/**/*\" ./src/assets/themes",
    "copy-themes-saga": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/saga-*/**/*\" ./src/assets/themes",
    "copy-themes-vela": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/vela-*/**/*\" ./src/assets/themes",
    "fix-line-endings": "node fix-line-endings.js",
    "build": "npm run clean && npm run copy-themes && ng build && npm run fix-line-endings",
    "start": "npm run copy-themes && ng serve --host 0.0.0.0 --proxy-config proxy.localhost.conf.json",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.1.0",
    "@angular/cdk": "14.1.0",
    "@angular/common": "14.1.0",
    "@angular/compiler": "14.1.0",
    "@angular/core": "14.1.0",
    "@angular/forms": "14.1.0",
    "@angular/platform-browser": "14.1.0",
    "@angular/platform-browser-dynamic": "14.1.0",
    "@angular/router": "14.1.0",
    "bootstrap": "4.6.2",
    "font-awesome": "4.7.0",
    "jquery": "3.6.0",
    "lodash-es": "4.17.21",
    "moment": "2.29.4",
    "natural-compare": "1.4.0",
    "ng-lazyload-image": "9.1.3",
    "ngx-countdown": "14.0.0",
    "ngx-mask": "14.0.3",
    "ngx-webstorage": "10.0.1",
    "primeicons": "5.0.0",
    "primeng": "14.0.0",
    "rxjs": "6.6.7",
    "strftime": "0.10.1",
    "tslib": "2.3.1",
    "zone.js": "0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.1.0",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/schematics": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "14.1.0",
    "@angular/compiler-cli": "14.1.0",
    "@types/jasmine": "3.10.6",
    "@types/jasminewd2": "2.0.10",
    "@types/jquery": "3.5.14",
    "@types/lodash-es": "4.17.6",
    "@types/natural-compare": "1.4.1",
    "@types/node": "16.11.43",
    "@types/strftime": "0.9.4",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "concurrently": "7.3.0",
    "copyfiles": "2.4.1",
    "eslint": "8.20.0",
    "husky": "7.0.4",
    "jasmine-core": "3.99.1",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.2",
    "karma-jasmine-html-reporter": "1.7.0",
    "line-ending-corrector": "1.0.1",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": "16.15.1"
  },
  "volta": {
    "node": "16.15.1"
  }
}
