{
  "name": "autosubliminal",
  "description": "Auto-Subliminal, the automated subliminal subtitle downloader",
  "author": "h3llrais3r",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h3llrais3r/Auto-Subliminal.git"
  },
  "homepage": "https://github.com/h3llrais3r/Auto-Subliminal#readme",
  "bugs": {
    "url": "https://github.com/h3llrais3r/Auto-Subliminal/issues"
  },
  "keywords": [
    "subtitle",
    "subtitles",
    "video",
    "movie",
    "episode",
    "tv",
    "show"
  ],
  "scripts": {
    "prepare": "cd ../.. && husky",
    "clean": "rimraf static",
    "copy-themes": "concurrently -n w: npm:copy-themes-*",
    "copy-themes-bootstrap4": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/bootstrap4-*/theme.css\" ./src/assets/themes",
    "copy-themes-arya": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/arya-*/**/*\" ./src/assets/themes",
    "copy-themes-lara": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/lara-*/**/*\" ./src/assets/themes",
    "copy-themes-saga": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/saga-*/**/*\" ./src/assets/themes",
    "copy-themes-vela": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/vela-*/**/*\" ./src/assets/themes",
    "fix-line-endings": "node fix-line-endings.js",
    "start": "npm run copy-themes && ng serve --host 0.0.0.0 --proxy-config proxy.localhost.conf.json",
    "build": "npm run clean && npm run copy-themes && ng build && npm run fix-line-endings",
    "build-ci": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "ng": "ng"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.12",
    "@angular/cdk": "17.3.10",
    "@angular/common": "17.3.12",
    "@angular/compiler": "17.3.12",
    "@angular/core": "17.3.12",
    "@angular/forms": "17.3.12",
    "@angular/platform-browser": "17.3.12",
    "@angular/platform-browser-dynamic": "17.3.12",
    "@angular/router": "17.3.12",
    "bootstrap": "4.6.2",
    "font-awesome": "4.7.0",
    "jquery": "3.7.1",
    "lodash-es": "4.17.21",
    "moment": "2.30.1",
    "natural-compare": "1.4.0",
    "ng-lazyload-image": "9.1.3",
    "ngx-countdown": "17.1.1",
    "ngx-mask": "17.1.8",
    "ngx-webstorage": "13.0.1",
    "ngx-window-token": "7.0.0",
    "primeicons": "7.0.0",
    "primeng": "17.18.11",
    "rxjs": "7.8.1",
    "strftime": "0.10.3",
    "tslib": "2.6.3",
    "zone.js": "0.14.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.10",
    "@angular-eslint/builder": "17.5.3",
    "@angular-eslint/eslint-plugin": "17.5.3",
    "@angular-eslint/eslint-plugin-template": "17.5.3",
    "@angular-eslint/schematics": "17.5.3",
    "@angular-eslint/template-parser": "17.5.3",
    "@angular/cli": "17.3.10",
    "@angular/compiler-cli": "17.3.12",
    "@types/jasmine": "4.6.4",
    "@types/jasminewd2": "2.0.13",
    "@types/jquery": "3.5.31",
    "@types/lodash-es": "4.17.12",
    "@types/natural-compare": "1.4.3",
    "@types/node": "18.19.56",
    "@types/strftime": "0.9.8",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "concurrently": "9.0.1",
    "copyfiles": "2.4.1",
    "eslint": "8.57.1",
    "husky": "9.1.6",
    "jasmine-core": "4.6.1",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.2",
    "karma-jasmine-html-reporter": "1.7.0",
    "line-ending-corrector": "1.0.1",
    "rimraf": "5.0.10",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": "18.20.4"
  },
  "volta": {
    "node": "18.20.4"
  }
}
