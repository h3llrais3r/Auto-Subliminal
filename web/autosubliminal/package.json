{
  "name": "autosubliminal",
  "description": "Auto-Subliminal, the automated subliminal subtitle downloader",
  "author": "h3llrais3r",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h3llrais3r/Auto-Subliminal.git"
  },
  "homepage": "https://github.com/h3llrais3r/Auto-Subliminal#readme",
  "bugs": {
    "url": "https://github.com/h3llrais3r/Auto-Subliminal/issues"
  },
  "keywords": [
    "subtitle",
    "subtitles",
    "video",
    "movie",
    "episode",
    "tv",
    "show"
  ],
  "scripts": {
    "prepare": "cd ../.. && husky install .githooks",
    "clean": "rimraf static",
    "copy-themes": "concurrently -n w: npm:copy-themes-*",
    "copy-themes-bootstrap4": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/bootstrap4-*/theme.css\" ./src/assets/themes",
    "copy-themes-arya": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/arya-*/theme.css\" ./src/assets/themes",
    "copy-themes-saga": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/saga-*/theme.css\" ./src/assets/themes",
    "copy-themes-vela": "copyfiles -u 4 \"./node_modules/primeng/resources/themes/vela-*/theme.css\" ./src/assets/themes",
    "fix-line-endings": "node fix-line-endings.js",
    "build": "npm run clean && npm run copy-themes && ng build --base-href=/autosubliminal/ && npm run fix-line-endings",
    "start": "npm run copy-themes && ng serve --host 0.0.0.0 --base-href=/autosubliminal/ --proxy-config proxy.localhost.conf.json",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.15",
    "@angular/cdk": "12.2.13",
    "@angular/common": "12.2.15",
    "@angular/compiler": "12.2.15",
    "@angular/core": "12.2.15",
    "@angular/forms": "12.2.15",
    "@angular/platform-browser": "12.2.15",
    "@angular/platform-browser-dynamic": "12.2.15",
    "@angular/router": "12.2.15",
    "bootstrap": "4.6.1",
    "font-awesome": "4.7.0",
    "jquery": "3.6.0",
    "lodash-es": "4.17.21",
    "moment": "2.29.1",
    "natural-compare": "1.4.0",
    "ng-lazyload-image": "9.1.2",
    "ngx-countdown": "12.0.1",
    "ngx-mask": "12.0.0",
    "ngx-webstorage": "8.0.0",
    "primeicons": "5.0.0",
    "primeng": "12.2.3",
    "rxjs": "6.6.7",
    "strftime": "0.10.1",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.15",
    "@angular-eslint/builder": "12.7.0",
    "@angular-eslint/eslint-plugin": "12.7.0",
    "@angular-eslint/eslint-plugin-template": "12.7.0",
    "@angular-eslint/schematics": "12.7.0",
    "@angular-eslint/template-parser": "12.7.0",
    "@angular/cli": "12.2.15",
    "@angular/compiler-cli": "12.2.15",
    "@types/jasmine": "3.10.3",
    "@types/jasminewd2": "2.0.10",
    "@types/jquery": "3.5.13",
    "@types/lodash-es": "4.17.5",
    "@types/natural-compare": "1.4.1",
    "@types/node": "12.20.37",
    "@types/strftime": "0.9.4",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "concurrently": "7.0.0",
    "copyfiles": "2.4.1",
    "eslint": "7.32.0",
    "husky": "7.0.4",
    "jasmine-core": "3.99.0",
    "karma": "6.3.12",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "line-ending-corrector": "1.0.1",
    "rimraf": "3.0.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.5"
  },
  "engines": {
    "node": "12.20.0"
  }
}
